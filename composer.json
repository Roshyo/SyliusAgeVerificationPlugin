{
    "name": "roshyo/age-verification-plugin",
    "type": "sylius-plugin",
    "description": "Provides a way to check customers age in Sylius.",
    "license": "MIT",
    "authors": [
        {
            "name": "BeHappy Communication",
            "role": "Organization",
            "homepage": "http://www.behappycom.com",
            "email": "projets@behappycom.com"
        },
        {
            "name": "St√©phane DECOCK",
            "role": "Developer",
            "homepage": "http://www.behappycom.com",
            "email": "s.decock@behappycom.com"
        }
    ],
    "require": {
        "php": "^7.4",

        "sylius/sylius": "^1.7",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.2"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/mink": "^1.7",
        "friends-of-behat/mink-browserkit-driver": "^1.3",
        "friends-of-behat/mink-extension": "^2.2",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "lchrusciel/api-test-case": "^3.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "0.12.3",
        "phpstan/phpstan-doctrine": "0.12.3",
        "phpstan/phpstan-webmozart-assert": "0.12.0",
        "phpunit/phpunit": "^7.0",
        "sensiolabs/security-checker": "^6.0",
        "stripe/stripe-php": "^6.28",
        "sylius-labs/coding-standard": "^3.0",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BeHappy\\SyliusAgeVerificationPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "auto-scripts": {
            "security-checker security:check": "script"
        }
    }
}
